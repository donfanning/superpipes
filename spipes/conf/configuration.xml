<configuration>
    <nodes>
        <node>
            <id>generator</id>
            <type>com.vaushell.spipes.nodes.stub.N_PostGenerator</type>
            <param name="frequency" value="3000" />
        </node>
        <node>
            <id>receptor</id>
            <type>com.vaushell.spipes.nodes.stub.N_MessageLogger</type>
