<?xml version="1.0" encoding="ISO-8859-1" ?>
<configuration>
    <general>
        <param name="datas-directory" value="data" />
    </general>
    <nodes>
        <node>
            <id>rss-vauchelles</id>
            <type>com.vaushell.spipes.nodes.rss.N_RSS</type>
            <param name="url" value="http://feeds.feedburner.com/fabienvauchelles" />
            <param name="delay" value="2000" />
        </node>
        <node>
            <id>filter-done</id>
            <type>com.vaushell.spipes.nodes.filters.done.NF_Done</type>
        </node>
        <node>
            <id>filter-date-min</id>
            <type>com.vaushell.spipes.nodes.filters.date.NF_Date</type>
            <param name="date-min" value="11/11/2013 10:00" />
        </node>
        <node>
            <id>receptor</id>
            <type>com.vaushell.spipes.nodes.stub.N_MessageLogger</type>
        </node>
    </nodes>
    <routes>
        <route>
            <source>rss-vauchelles</source>
            <destination>filter-date-min</destination>
        </route>
        <route>
            <source>filter-date-min</source>
            <destination>filter-done</destination>
        </route>
        <route>
            <source>filter-done</source>
            <destination>receptor</destination>
        </route>
    </routes>
</configuration>
